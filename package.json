{
  "name": "@bifravst/device-ui",
  "version": "0.0.0-development",
  "description": "Web app to simulate a Cat Tracker",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/device-ui.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/device-ui/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "Bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "LICENSE",
  "dependencies": {
    "bootstrap": "4.5.3",
    "inter-ui": "3.15.0",
    "leaflet": "1.7.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-leaflet": "3.0.2",
    "reactstrap": "8.7.1",
    "snyk": "1.425.3",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.75",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@prefresh/snowpack": "2.1.0",
    "@snowpack/plugin-babel": "2.1.4",
    "@snowpack/plugin-react-refresh": "2.3.7",
    "@snowpack/plugin-typescript": "1.1.0",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-leaflet": "2.5.2",
    "@types/reactstrap": "8.7.2",
    "@types/snowpack-env": "2.3.2",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.13.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "snowpack": "2.17.1",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "snyk": true
}
