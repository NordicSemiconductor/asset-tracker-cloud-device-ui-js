{
  "name": "@nordicsemiconductor/asset-tracker-cloud-device-ui",
  "version": "0.0.0-development",
  "description": "Web app to simulate a Cat Tracker",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cp -rv node_modules/inter-ui/Inter\\ \\(web\\)/* public/inter",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-device-ui-js#readme",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "LICENSE",
  "dependencies": {
    "bootstrap": "5.1.3",
    "inter-ui": "3.19.3",
    "leaflet": "1.7.1",
    "mcc-mnc-list": "^1.1.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "3.2.2",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.2.16",
    "@prefresh/snowpack": "3.1.2",
    "@snowpack/plugin-babel": "2.1.7",
    "@snowpack/plugin-react-refresh": "2.5.0",
    "@snowpack/plugin-typescript": "1.2.1",
    "@types/classnames": "2.3.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@types/react-leaflet": "2.8.2",
    "@types/snowpack-env": "2.3.4",
    "@types/styled-components": "5.1.15",
    "babel-eslint": "10.1.0",
    "call-bind": "*",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "7.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "snowpack": "3.8.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
