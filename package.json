{
  "name": "@bifravst/device-ui",
  "version": "0.0.0-development",
  "description": "Web app to simulate a Cat Tracker",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/device-ui.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/device-ui/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "LICENSE",
  "dependencies": {
    "leaflet": "1.7.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-leaflet": "2.7.0",
    "reactstrap": "8.6.0",
    "snyk": "1.401.0",
    "styled-components": "5.2.0"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.97",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/classnames": "2.2.10",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-leaflet": "2.5.2",
    "@types/reactstrap": "8.5.1",
    "@types/styled-components": "5.1.3",
    "babel-eslint": "10.1.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "firebase-tools": "8.11.2",
    "husky": "4.3.0",
    "react-scripts": "3.4.3",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "snyk": true
}
