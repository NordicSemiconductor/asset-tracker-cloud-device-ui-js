{
  "name": "@bifravst/device-ui",
  "version": "0.0.0-development",
  "description": "Web app to simulate a Cat Tracker",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/device-ui.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/device-ui/issues"
  },
  "homepage": "https://bifravst.github.io/",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "bootstrap": "^4.3.1",
    "leaflet": "^1.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-leaflet": "^2.4.0",
    "reactstrap": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-leaflet": "^2.2.1",
    "@types/reactstrap": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-app": "^5.0.1",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-scripts": "^3.0.1",
    "typescript": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx",
      "git add"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
